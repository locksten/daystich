{
  "name": "daystich",
  "description": "Time tracking app",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "private": true,
  "author": "locksten <email@locksten.com>",
  "homepage": "https://daystich.locksten.com",
  "bugs": "https://github.com/locksten/daystich/issues",
  "repository": "github:locksten/daystich",
  "keywords": [
    "timetracker",
    "react"
  ],
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 --ci -s public",
    "build-storybook": "build-storybook -s public"
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@reach/dialog": "^0.10.3",
    "@react-aria/focus": "^3.1.0",
    "@reduxjs/toolkit": "^1.3.6",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/humanize-duration": "^3.18.0",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.42",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/react-select": "^3.0.13",
    "date-fns": "^2.14.0",
    "emotion-theming": "^10.0.27",
    "humanize-duration": "^3.23.1",
    "re-reselect": "^4.0.0",
    "react": "^16.13.1",
    "react-color": "^2.18.1",
    "react-cool-onclickoutside": "^1.5.2",
    "react-dnd": "^11.1.3",
    "react-dnd-html5-backend": "^11.1.3",
    "react-dom": "^16.13.1",
    "react-hook-form": "^6.3.0",
    "react-modal": "^3.11.2",
    "react-redux": "^7.2.0",
    "react-scripts": "3.4.1",
    "react-select": "^3.1.0",
    "redux-persist": "^6.0.0",
    "reselect": "^4.0.0",
    "twin.macro": "^1.7.0",
    "typescript": "^3.7.5"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/preset-create-react-app": "^3.0.0",
    "@storybook/react": "^5.3.19",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-emotion": "^10.0.27",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.7",
    "prettier": "^2.0.5",
    "react-icons": "^3.10.0"
  },
  "babelMacros": {
    "twin": {
      "config": "src/styling/tailwind.config.js",
      "preset": "emotion",
      "debugProp": true,
      "debugPlugins": false,
      "debug": false
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{json,js,ts,tsx}": [
      "eslint --config .custom-eslintrc.json --max-warnings 0",
      "prettier --write"
    ]
  },
  "eslintConfig": {
    "extends": "react-app",
    "overrides": [
      {
        "files": [
          "**/*.ts?(x)"
        ],
        "rules": {
          "@typescript-eslint/no-unused-vars": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
